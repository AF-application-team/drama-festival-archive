<Window x:Class="AF_Desktop_Application.PropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PropertiesWindow" Height="385" Width="525">
    <DockPanel>
        <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Content="Anuluj" IsCancel="True" Margin="5,0,5,0"/>
            <Button Content="Zapisz" IsDefault="True" />
        </StackPanel>
        <DataGrid x:Name="DataGrid" HeadersVisibility="Row">
            <DataGrid.Columns>
                <DataGridTextColumn Width="*" Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="Parametr"/>
                <DataGridTextColumn Width="3*" Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="Wartość"/>
            </DataGrid.Columns>
        </DataGrid>

    </DockPanel>
</Window>
